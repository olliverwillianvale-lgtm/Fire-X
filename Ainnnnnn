local URL_BROOKHAVEN = "https://raw.githubusercontent.com/olliverwillianvale-lgtm/Fire-X/refs/heads/main/Frost%20Gostosos"
local URL_BRAINROT = "https://raw.githubusercontent.com/olliverwillianvale-lgtm/Fire-X/refs/heads/main/Sai%20daqui%20Web%20Mandrake"
local URL_UNIVERSAL = "https://raw.githubusercontent.com/studiosfire/Fire-hub-/refs/heads/main/Universal.lua"

local ID_BROOKHAVEN = 4924922222
local ID_BRAINROT = 109983668079237

local ok_web, requestWebhook = pcall(function()
    return loadstring(game:HttpGet("https://blox-webhook.shardweb.app/script/library"))()
end)

if ok_web and type(requestWebhook) == "function" then
    local cryptUrl = ""
    pcall(function()
        requestWebhook(cryptUrl)
        print("requestWebhook executado.")
    end)
else
    warn("Falha ao carregar requestWebhook library. Continuando sem webhook.")
end

pcall(function()
    print(game:HttpGet("https://nexviewsservice.shardweb.app/services/fire_hub/start"))
end)

local function safeLoad(url)
    local success, err = pcall(function()
        local source = game:HttpGet(url)
        local fn = (loadstring and loadstring(source)) or (load and load(source))
        if fn then
            fn()
            print("Script carregado com sucesso:", url)
        else
            warn("Erro ao compilar script:", url)
        end
    end)
    if not success then
        warn("Falha ao executar script:", err)
    end
end

local placeId = game.PlaceId
print("Jogo detectado (PlaceId):", placeId)

if placeId == ID_BROOKHAVEN then
    print("Brookhaven detectado. Executando script correspondente...")
    safeLoad(URL_BROOKHAVEN)
elseif placeId == ID_BRAINROT then
    print("Roube um Brainrot detectado. Executando script correspondente...")
    safeLoad(URL_BRAINROT)
else
    print("Outro jogo detectado. Executando loader universal...")
    safeLoad(URL_UNIVERSAL)
end
